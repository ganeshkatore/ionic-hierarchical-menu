{
  "name": "ng-hierarchical-menu",
  "description": "Hierarchical menu is a hierarchical / multi-level menu component for Angular 2+",
  "version": "0.1.0",
  "scripts": {
    "ngc": "ngc",
    "webpack": "webpack",
    "build": "rm -rf dist/ && ngc && webpack",
    "serve": "http-server ./dist -a 0.0.0.0 -p 9000",
    "dist": "npm run build && cp package.json dist/ && cp README.md dist/",
    "pack": "npm run dist && npm pack dist/",
    "test": "npm run build && karma start",
    "test-watch": "ngc && karma start --no-single-run --auto-watch",
    "posttest": "node_modules/.bin/remap-istanbul -i coverage/coverage.json -o coverage -t html",
    "coverage": "http-server -c-1 -o -p 9875 ./coverage"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/moberwasserlechner/ng-hierarchical-menu.git"
  },
  "keywords": [
    "angular",
    "angular2",
    "menu",
    "vertical nav",
    "navigation",
    "multi-level"
  ],
  "author": "Michael Oberwasserlechner",
  "homepage": "https://github.com/moberwasserlechner/ng-hierarchical-menu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/moberwasserlechner/ng-hierarchical-menu/issues"
  },
  "main": "bundles/index.umd.js",
  "module": "index.js",
  "typings": "index.d.ts",
  "peerDependencies": {
    "@angular/core": "^2.4.8"
  },
  "devDependencies": {
    "@angular/common": "^2.4.8",
    "@angular/compiler": "^2.4.8",
    "@angular/compiler-cli": "^2.4.8",
    "@angular/core": "^2.4.8",
    "@angular/platform-browser": "^2.4.8",
    "@angular/platform-browser-dynamic": "^2.4.8",
    "@angular/platform-server": "^2.4.8",
    "@types/jasmine": "^2.5.37",
    "@types/node": "6.0.46",
    "awesome-typescript-loader": "2.2.4",
    "cash-cp": "^0.2.0",
    "codelyzer": "2.0.0-beta.2",
    "commitizen": "2.8.6",
    "core-js": "2.4.1",
    "cz-conventional-changelog": "1.2.0",
    "http-server": "^0.9.0",
    "istanbul-instrumenter-loader": "0.2.0",
    "jasmine-core": "^2.5.2",
    "karma": "^1.5.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-firefox-launcher": "^1.0.1",
    "karma-jasmine": "^1.1.0",
    "karma-mocha-reporter": "^2.2.2",
    "karma-coverage": "^1.1.1",
    "karma-remap-coverage": "^0.1.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.3",
    "loader-utils": "~0.2.16",
    "reflect-metadata": "^0.1.3",
    "remap-istanbul": "^0.9.1",
    "rxjs": "^5.0.1",
    "semantic-release": "4.3.5",
    "source-map-loader": "0.1.5",
    "ts-helpers": "1.1.2",
    "tslint": "~4.0.0",
    "tslint-loader": "3.3.0",
    "typescript": "~2.0.9",
    "webpack": "~2.2.0",
    "zone.js": "^0.7.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
